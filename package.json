{
    "name": "space-eye",
    "version": "1.2.7",
    "description": "Live satellite imagery for your desktop background",
    "main": "./dist/main.js",
    "scripts": {
        "build": "gulp build",
        "build-ci": "gulp build-ci",
        "start-dev": "gulp start-dev",
        "prestart": "npm run build",
        "start": "electron .",
        "lint": "eslint --ext=jsx,js,tsx,ts src",
        "test": "jest '(\\/test\\/(?!e2e/)).*'",
        "pretest:e2e": "npm run build",
        "test:e2e": "jest '(\\/test\\/e2e/).*'",
        "pack": "npm run build && electron-builder --dir",
        "dist": "gulp dist",
        "postinstall": "electron-builder install-app-deps"
    },
    "lint-staged": {
        "{src,test,mocks}/**/*.{json,css,scss,md}": [
            "prettier --config ./.prettierrc --write"
        ],
        "{src,test,mocks}/**/*.{js,ts,tsx}": [
            "prettier --config ./.prettierrc --write",
            "eslint --ext=jsx,js,ts,tsx --fix src"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/KYDronePilot/SpaceEye.git"
